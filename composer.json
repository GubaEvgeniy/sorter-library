{
  "name": "whoisthere/sorter-library",
  "description": "This is simple library for sort arrays.",
  "type": "library",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Evgeniy Guba",
      "email": "evgeniyguba@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "App\\Sorter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Sorter\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
